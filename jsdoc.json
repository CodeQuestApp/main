{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "source": {
        "include": ["Src/frontend/js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|Algorithmes|Specification)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "encoding": "utf8",               
        "destination": "./docs",          
        "recurse": true
    }
}